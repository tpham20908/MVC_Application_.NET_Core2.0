@model ICollection<Contoso.Entities.Person>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>@Model.Count People from @ViewBag.from to @ViewBag.to</h4>
<hr />

@foreach (var p in Model)
{
    <dl class="dl-horizontal">
        <dt><b>Name: </b></dt>
        <dd>@p.FirstName @p.MiddleName @p.LastName</dd>
    </dl>
    <hr />
}